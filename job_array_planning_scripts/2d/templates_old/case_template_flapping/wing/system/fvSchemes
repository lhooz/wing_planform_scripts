FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

gradSchemes
{
    default 	     Gauss linear;
}

divSchemes
{
    default          Gauss linear;
}

laplacianSchemes
{
    default         Gauss linear corrected;
}

